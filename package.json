{
  "name": "millermakers",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build:dev": "ng build --stats-json",
    "build:prod": "ng build --prod --aot --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "verify": "superstatic --compression --config superstatic.json ./dist",
    "analyze": "webpack-bundle-analyzer ./dist/stats.json",
    "deploy": "aws s3 sync dist  s3://millermakers.com --profile PERSONAL_MM --delete && npm run invalidate",
    "invalidate": "aws cloudfront --profile PERSONAL_MM create-invalidation --distribution-id E26RN2GSM20KG1 --paths '/index.html'"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.800.4",
    "@angular/animations": "~8.0.2",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.2",
    "@angular/compiler": "~8.0.2",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.0.2",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~8.0.2",
    "@angular/platform-browser-dynamic": "~8.0.2",
    "@angular/router": "~8.0.2",
    "@ngu/carousel": "^1.5.4",
    "@ngx-gallery/core": "^3.3.1",
    "@ngx-gallery/gallerize": "^3.3.1",
    "@ngx-gallery/lightbox": "^3.3.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "instafeed.js": "^1.4.1",
    "rxjs": "^6.3.3",
    "typed.js": "^2.0.9",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.2",
    "@angular/language-service": "~8.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "imageoptim-cli": "^2.3.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "superstatic": "^6.0.3",
    "tinypng-cli": "0.0.7",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.1",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
